/* pages/details/wyjx/wyjx.wxss */
.bg{
  width: 100%;
  height: 100vh;
  overflow: hidden;
  background-image: -webkit-linear-gradient(0deg, #47DA7C, #41EAC0); 
}
.jx{
  width: 558rpx;
  margin: 0 auto;
  margin-top:80rpx; 
  position: relative;
}
.xie_num{
  position: absolute;
  left: 40%;
  top: 68%;
}
.xie_num image{
  margin: 0 20rpx;
  vertical-align: middle;
}
.xie_num text{
  font-size:32rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.juanzeng{
  width: 185rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 20rpx;
  background-image: -webkit-linear-gradient(0deg, #47DA7C, #41EAC0); 
  position: absolute;
  left: 0;
  right: 0;
  bottom: 40rpx;
  margin: auto;
  text-align: center;
  font-size:24rpx;
  font-weight:bold;
  color:rgba(255,255,255,1);
}
.select{
  text-align: center;
  margin-top: 20rpx;
}
.select icon{
  vertical-align: middle;
  margin-right: 8rpx;
}
.select text{
  font-size:28rpx;
  color:rgba(255,255,255,1);
}
navigator{
  display:inline-block;
}
.jxlb{
  display: block;
  color: #fff;
  margin: 0 auto;
  font-size:30rpx;
  width: 200rpx;
  height: 60rpx;
  border:1px solid #fff;
  border-radius: 20rpx;
  text-align: center;line-height: 60rpx;
  margin-top: 20rpx;
}
.my-check{
  vertical-align: middle;
  /* float: left;
  margin-top:90rpx; 
  margin-right: 30rpx; */
  width: 43rpx;
  height: 43rpx;
  margin-right: 20rpx;
}