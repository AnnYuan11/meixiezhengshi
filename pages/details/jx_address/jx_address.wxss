/* pages/details/jx_address/jx_address.wxss */
.list{
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 1px solid #E6E6E6;
  padding: 0 30rpx;
  box-sizing: border-box;
  font-size:30rpx;
  font-weight:400;
  color:rgba(51,51,51,1);
}
.address{
  margin-left: 30rpx;
}
/* 浮层 */
.cover{
  width: 100%;
  height: 100%;
  background:rgba(0,0,0,0.7);
  position: absolute;
  top: 0;
  z-index: 9999;
  overflow: hidden;
}
.yuyue{
  width: 343rpx;
  position: relative;
  left: 0;
  right: 0;
  margin: auto;
  top:350rpx;
}
.img{
  width: 343rpx;
  height: 337rpx;
}
.yuyue text{
  font-size:24rpx;
  color:rgba(51,51,51,1);
  position: absolute;
  bottom: 30rpx;
  left: 60rpx;
}
.close{
  text-align: center;
  margin-top: 50%;
}
.img_close{
  width: 48rpx;
  height: 48rpx;
}