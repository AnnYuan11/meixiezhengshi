/* pages/login/login.wxss */
page{
  background: #F4F4F4;
}
.con{
  margin: 20rpx 32rpx;
  font-size: 24rpx;
  color: #999;
}
.content{
  width: 100%;
  background: #fff;
}
.list{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #E6E6E6;
  /* padding: 30rpx;
  box-sizing: border-box; */
}
.list:last-child{
  border: none;
}
.list .name{
  font-size: 30rpx;
  color: #333;
  margin-left: 30rpx;
  float: left;
}
.list .phone{
  width: 420rpx;
  height: 58rpx;
  line-height: 58rpx;
  float: left;
  color: #999;
  font-size: 30rpx;
  margin-top: 15rpx;
  margin-left: 40rpx;
}
.pla{
  font-size:30rpx;
  font-weight:400;
  color:rgba(153,153,153,1);
}
.miao{
  width: 140rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size:24rpx;
  font-weight:400;
  color:rgba(255,255,255,1);
  border-radius: 30rpx;
  text-align: center;
  background-image: -webkit-linear-gradient(0deg, #47DA7C, #41EAC0); 
  float: right;
  margin-top: 18rpx;
  margin-right: 30rpx;
}
.login{
  width: 383rpx;
  height: 80rpx;
  line-height: 80rpx;
  background-image: -webkit-linear-gradient(0deg, #47DA7C, #41EAC0); 
  font-size:30rpx;
  font-weight:bold;
  color:rgba(255,255,255,1);
  text-align: center;
  border-radius: 50rpx;
  margin: 68rpx auto;
}
.mask{
  width: 140rpx;
  height: 50rpx;
  line-height: 50rpx;
  border-radius: 30rpx;
  text-align: center;
  background:transparent; 
  float: right;
  margin-top: -47rpx;
  margin-right: 30rpx;
}
.select{
  text-align: center;
  margin-top: 20rpx;
}
.select icon{
  vertical-align: middle;
  margin-right: 8rpx;
}
.select text{
  font-size:28rpx;
  color:#47DA7C;
}
navigator{
  display:inline-block;
}
.my-check{
  vertical-align: middle;
  /* float: left;
  margin-top:90rpx; 
  margin-right: 30rpx; */
  width: 43rpx;
  height: 43rpx;
  margin-right: 20rpx;
}